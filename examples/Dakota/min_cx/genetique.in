environment,
    graphics
    tabular_graphics_data
    tabular_graphics_file = 'optim_res.dat'

method,
    max_iterations = 500
	max_function_evaluations = 10000
    coliny_ea
		population_size = 50
		fitness_type merit_function
		mutation_type offset_normal
		mutation_rate 1.0

variables,
    continuous_design = 6
    initial_point      0.0      0.0     0.4     0.4     0.12     0.12
    lower_bound        0.0      0.0     0.3     0.3     0.08     0.08
    upper_bound        0.05     0.05    0.5     0.5     0.15     0.15
    descriptors      'm_root' 'm_top' 'p_root' 'p_top' 't_root' 't_top'

interface,
    system
    analysis_driver = 'python myfunc.py'

responses,
    objective_functions = 1
    nonlinear_inequality_constraints = 1
    descriptors = 'my_function' 'my_constraint'
    # numerical_gradients
    no_gradients
    no_hessians
    #numerical_hessians
